class Bedroom2 extends MyHome{
Light room2_light_1= new Light (false,90.01, "warm white");
Light room2_light_2= new Light (false,87.07, "cool white");
Fan romm2_fan= = new Fan(False,4);
AC room2_ac= new AC(false,22,"cool");
Tv room2_tv=new TV(false,125);
Iron room2_iron=new Iron(false,77.0);
Printer room2_printer=new Printer(false);
Computer room2_computer= new Computer(false);
Speaker room2_speaker= new Speaker(false,50);

  Bedroom2(String name_val){
    super(name_val);
}

public void display(){
System.out.println(" Bedroom 2 appliance status:");
System.out.println("Light 1 brightness :"+room2_light_1.brightness);
System.out.println("Light 2 brightness :"+room2_light_2.brightness);
System.out.println("fan speed :"+room2_fan.speed);
System.out.println("AC temperature :"+room2_ac.temperature);
System.out.println("Tv volume:"+room2_tv.volume);
System.out.println("iron temperature" +room2_iron.temperature);
System.out.println("Speaker volume :" + room2_speaker.volume);
}

}





